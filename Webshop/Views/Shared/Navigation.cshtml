@using EPiServer
@using EPiServer.Core

@{
    var cr = EPiServer.ServiceLocation.ServiceLocator.Current.GetInstance<IContentRepository>();
    var childPages = cr.GetChildren<PageData>(ContentReference.StartPage);
}

@Html.PageLink(ContentReference.StartPage)

@foreach (var page in childPages)
{
    <b>:: @Html.PageLink(page)</b>
}