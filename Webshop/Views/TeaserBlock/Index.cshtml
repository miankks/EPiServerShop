@model Webshop.Models.Blocks.TeaserBlock

<div class="row">
    <div class="media">
        <div class="mediaImg">
            <img src="@Url.ContentUrl(Model.TeaserImage)"/>
        </div>
        <div class="mediaText">
            <h2>@Model.TeaserHeading</h2>
            <p>@Html.PropertyFor(x => x.TeaserText)</p>
            Pris: <h2>@Html.PropertyFor(x => x.TeaserPrice)</h2>
            <h2> @Html.PropertyFor(x => x.TeaserLink)</h2>
        </div>
    </div>
</div>